    "clsx": "^2.1.0",
    "flatgeobuf": "^3.27.1",
    "leaflet": "^1.9.4",
    "next": "14.0.4",
    "next-swagger-doc": "^0.4.0",
    "nuqs": "^1.17.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-leaflet": "^4.2.1",
    "react-map-gl": "^7.1.7",
    "sass": "^1.72.0",
    "swagger-ui-react": "^5.10.5"
