{
  "name": "@e-chan1007/uec-map-server",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev": "run-p prebuild dev:*",
    "build": "run-s prebuild && next build",
